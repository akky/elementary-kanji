{
  "name": "elementary-kanji",
  "description": "Show Kanji leaned at Japanese elementary school by grade",
  "version": "1.0.0",
  "private": true,
  "files": [
    "*.js"
  ],
  "scripts": {
    "scrape": "node scripts/scraper/scraper.js",
    "lint": "eslint scripts/*.js && stylelint css/*.css",
    "beautify": "eslint --fix scripts/*.js && stylelint --fix css/*.css",
    "check_firefox_compatibility": "wemf ./manifest.json --validate",
    "test": "karma start karma.conf.js",
    "prepack": "zip -r extension.zip manifest.json scripts/*.js css/*.css data/*.js _locales icons/*.png",
    "pack": "yarn run prepack",
    "update_package_json": "node_modules/.bin/syncyarnlock -s -k"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/eslint-parser": "7.19.1",
    "chai": "4.3.7",
    "chrome-webstore-upload-cli": "^2.2.2",
    "eslint": "^8.57.0",
    "jsdom": "^20.0.3",
    "karma": "6.4.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage": "2.2.0",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-mocha": "2.0.1",
    "mocha": "^10.8.2",
    "nth-check": "^2.1.1",
    "scrape-it": "^5.3.2",
    "stylelint": "^14.16.1",
    "syncyarnlock": "^1.0.19",
    "web-ext": "^7.12.0",
    "wemf": "^1.3.0"
  },
  "author": "Akky Akimoto",
  "license": "MIT",
  "main": "background.js",
  "directories": {
    "test": "test"
  },
  "keywords": []
}
